{
  "author": "igorlogius",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "description": "create a qrcode from any tab url, link, image or text",
  "homepage_url": "https://github.com/igorlogius/show-qrcode",
  "icons": {
    "184": "icon.png"
  },
  "manifest_version": 2,
  "name": "Show QRcode",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "menus"
  ],
  "options_ui": {
    "page": "options.html"
  },
  "version": "1.5.24"
}
