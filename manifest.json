{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/show-qrcode",
  "background": {
    "scripts": ["background.js"]
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F10"
      }
    }
  },
  "icons": {
    "184": "icon.png"
  },
  "manifest_version": 2,
  "name": "Show QRcode",
  "browser_action": {
    "default_area": "navbar",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage", "menus"],
  "optional_permissions": ["bookmarks"],
  "options_ui": {
    "page": "options.html"
  },
  "version": "1.8.30"
}
